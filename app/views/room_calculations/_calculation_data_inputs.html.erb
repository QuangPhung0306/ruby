<div class="calculation_tabs">
  <div class="box_gr clearfix flex-direction-column" id="anchor_to_scroll">
    <div class="w-tabscroll-ed margin-bottom-25">
      <ul class="center slider css-autow">
        <li class="item" data-index="0">
          <%= link_to "#tabs_ct_drc", class: "nav-link active", data: {toggle: "pill"}, role: "tab" do %>
            <span><%= t(".desired_room_climate") %></span>
          <% end %>
        </li>
        <li class="item" data-index="1">
          <%= link_to "#tabs_ct_odc", class: "nav-link", data: {toggle: "pill"}, role: "tab" do %>
            <span><%= t(".outdoor_design_climate") %></span>
          <% end %>
        </li>
        <li class="item" data-index="2">
          <%= link_to "#tabs_ct_src", class: "nav-link", data: {toggle: "pill"}, role: "tab" do %>
            <span><%= t(".surrounding_rooms_climate") %></span>
          <% end %>
        </li>
        <li class="item" data-index="3">
          <%= link_to "#tabs_ct_vii", class: "nav-link", data: {toggle: "pill"}, role: "tab" do %>
            <span><%= t(".ventilation_including_infiltration") %></span>
          <% end %>
        </li>
        <li class="item" data-index="4">
          <%= link_to "#tabs_ct_pitr", class: "nav-link", data: {toggle: "pill"}, role: "tab" do %>
            <span><%= t(".personnel_in_the_room") %></span>
          <% end %>
        </li>
        <li class="item" data-index="5">
          <%= link_to "#tabs_ct_ows", class: "nav-link", data: {toggle: "pill"}, role: "tab" do %>
            <span><%= t(".open_water_surface") %></span>
          <% end %>
        </li>
        <li class="item" data-index="6">
          <%= link_to "#tabs_ct_pd", class: "nav-link", data: {toggle: "pill"}, role: "tab" do %>
            <span><%= t(".product_drying") %></span>
          <% end %>
        </li>
        <li class="item" data-index="7">
          <%= link_to "#tabs_ct_other", class: "nav-link", data: {toggle: "pill"}, role: "tab" do %>
            <span><%= t(".other") %></span>
          <% end %>
        </li>
        <li class="item" data-index="8">
          <%= link_to "#tabs_ct_tml", class: "nav-link", data: {toggle: "pill"}, role: "tab" do %>
            <span><%= t(".total_moisture_load") %></span>
          <% end %>
        </li>
        <li class="item" data-index="9">
          <%= link_to "#tab_abcdeghfj", class: "nav-link", id: "pill_tab_abcdeghfj", data: {toggle: "pill"}, role: "tab" do %>
            <span><%= t ".dehumidifying" %></span>
          <% end %>
        </li>
      </ul>

      <ul class="nav nav-pills nav-scroll nav_tabs_ed" id="pills-tab" role="tablist">
      </ul>
    </div>

    <div class="w-btn-total flex-order-default">
      <div class="text-center">
        <a class="btn btnPrevious">Previous</a>
        <a class="btn btnNext">Next</a>
      </div>
      <div class="text-center mt-3 js-total-tabs">
        (<span class="js-tabs-default">1</span>/<span class="js-tabs-nb">18</span>)
      </div>
    </div>

    <hr>
    <div class="tab-content tab_ct_ed margin-bottom-0" id="pills-tabContent">
      <div class="tab-pane fade show active" id="tabs_ct_drc" role="tabpanel" aria-labelledby="tabs_ct_drc">
        <%= render "calculation_drc", project: project %>
      </div>
      <div class="tab-pane fade" id="tabs_ct_odc" role="tabpanel" aria-labelledby="tabs_ct_odc">
        <%= render "calculation_odc", project: project %>
      </div>
      <div class="tab-pane fade" id="tabs_ct_src" role="tabpanel" aria-labelledby="tabs_ct_src">
        <%= render "calculation_src", project: project %>
      </div>
      <div class="tab-pane fade" id="tabs_ct_vii" role="tabpanel" aria-labelledby="tabs_ct_vii">
        <%= render "calculation_vii", project: project %>
      </div>
      <div class="tab-pane fade" id="tabs_ct_pitr" role="tabpanel" aria-labelledby="tabs_ct_pitr">
        <%= render "calculation_pitr", project: project %>
      </div>
      <div class="tab-pane fade" id="tabs_ct_ows" role="tabpanel" aria-labelledby="tabs_ct_ows">
        <%= render "calculation_ows", project: project %>
      </div>
      <div class="tab-pane fade" id="tabs_ct_pd" role="tabpanel" aria-labelledby="tabs_ct_pd">
        <%= render "calculation_pd", project: project %>
      </div>
      <div class="tab-pane fade" id="tabs_ct_other" role="tabpanel" aria-labelledby="tabs_ct_other">
        <%= render "calculation_other", project: project %>
      </div>
      <div class="tab-pane fade" id="tabs_ct_tml" role="tabpanel" aria-labelledby="tabs_ct_tml">
        <%= render "calculation_tml", project: project %>
      </div>


      <div class="tab-pane fade" id="tab_abcdeghfj" role="tabpanel" aria-labelledby="a">
        <%= render "calculation_cdej", project: project %>
        <%= render "calculation_a", project: project %>
        <%= render "calculation_hfg", project: project %>
        <%= render "calculation_b", project: project %>
      </div>
      <div class="tab-pane fade" id="tab_b" role="tabpanel" aria-labelledby="b">
      </div>
      <div class="tab-pane fade" id="tab_hfg" role="tabpanel" aria-labelledby="hfg">
      </div>
      <div class="tab-pane fade" id="tab_cdej" role="tabpanel" aria-labelledby="cdej">
      </div>
      <div class="tab-pane fade" id="tabs_ct2" role="tabpanel" aria-labelledby="tabs_ct2">
        <div class="box_gr_tlt">
          <h2 class="box_gr_hd"><%= t(".measurements") %></h2>
          <h2 class="box_gr_hd"><%= t(".units_of_measurement") %></h2>
          <h2 class="box_gr_hd"><%= t(".dimensioning") %></h2>
          <h2 class="box_gr_hd"><%= t(".average") %></h2>
        </div>
        <div class="box_gr_item">
          <div class="gr_item">
            <span><%= t(".temperature") %></span>
          </div>
          <div class="gr_item">
            <span><sup>o</sup>C</span>
          </div>
          <div class="gr_item">
            <span>25</span>
          </div>
          <div class="gr_item">
            <span>20</span>
          </div>
        </div>
        <div class="box_gr_item">
          <div class="gr_item">
            <span><%= t(".relative_humidity") %></span>
          </div>
          <div class="gr_item">
            <span>%RH</span>
          </div>
          <div class="gr_item">
            <input type="text" value="60">
          </div>
          <div class="gr_item">
            <input type="text" value="60">
          </div>
        </div>
        <div class="box_gr_item">
          <div class="gr_item">
            <span><%= t(".atmospheric_pressure") %></span>
          </div>
          <div class="gr_item">
            <span>mbar</span>
          </div>
          <div class="gr_item">
            <span>1014</span>
          </div>
          <div class="gr_item">
            <span>1014</span>
          </div>
        </div>
        <div class="box_gr_item">
          <div class="gr_item">
            <span><%= t(".moisture_content") %></span>
          </div>
          <div class="gr_item">
            <span>g/kg</span>
          </div>
          <div class="gr_item">
            <input type="text" value="11.9">
          </div>
          <div class="gr_item">
            <input type="text" value="8.7">
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
